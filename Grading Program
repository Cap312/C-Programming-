#include<stdio.h>
#include<math.h>
int main()
{
    int mark;
    printf("Enter your marks : \n");
    scanf("%d",&mark );
    if (mark<=100&&mark>=85)
        printf("Your Grade is : \nGrade A");
    else if (mark<=84&&mark>=70)
        printf("Your Grade is : \nGrade B");
    else if (mark<=69&&mark>=55)
        printf("Your Grade is : \nGrade C");
    else if (mark<=54&&mark>=40)
        printf("Your Grade is : \nGrade D");
    else if (mark<40)
        printf("Your Grade is : \nGrade F");
    else
        printf("Please enter marks within the range of 0 to 100.");
    return 0;
}
